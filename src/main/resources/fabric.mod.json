{
  "schemaVersion": 1,
  "id": "character_engine",
  "version": "${version}",
  "name": "CharacterEngine",
  "description": "YOMI Hustle but cubes",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/character_engine/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "dev.rosenoire.character_engine.data.CharacterEngineDataGenerator"
    ],
    "client": [
      "dev.rosenoire.character_engine.client.CharacterEngineClient"
    ],
    "main": [
      "dev.rosenoire.character_engine.common.CharacterEngine"
    ],
    "cardinal-components-entity": [
      "dev.rosenoire.character_engine.cca.ModEntityComponentIndex"
    ]
  },
